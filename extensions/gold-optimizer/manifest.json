{
  "manifest_version": 3,
  "name": "Otimizador Gold",
  "version": "1.0.1",
  "description": "Usa o index.html como popup da extens√£o.",
  "permissions": ["sidePanel"],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_title": "Otimizador Gold",
    "default_popup": "index.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "side_panel": {
    "default_path": "index.html"
  }
}
