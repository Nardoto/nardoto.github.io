{
    "manifest_version": 3,
    "name": "Google Labs Downloader",
    "version": "1.3.1",
    "description": "Automatically download VEO3 videos and Whisk images with custom names, skip duplicates, and individual download buttons from Google Labs",
    "permissions": ["activeTab"],
    "content_scripts": [
        {
            "matches": [
                "*://*.google/fx/*",
                "*://labs.google/fx/*"
            ],
            "js": ["src/content.js"]
        },
        {
            "matches": [
                "*://labs.google/fx/*/tools/whisk/*",
                "*://*.google/fx/*/tools/whisk/*",
                "*://whisk.google/*",
                "*://*.whisk.google/*"
            ],
            "js": ["src/content_whisk_enhanced.js"]
        }
    ],
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "action": {
        "default_icon": "icons/icon48.png"
    }
}