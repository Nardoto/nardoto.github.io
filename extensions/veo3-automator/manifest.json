{
  "manifest_version": 3,
  "name": "Google Labs automator",
  "version": "3.0",
  "description": "Automatiza envio de prompts e imagens para VEO3 da Google com Frame-to-Video. Por Nardoto.",
  "author": "Nardoto",
  "icons": {
    "16": "assets/icons/icon16.png",
    "48": "assets/icons/icon48.png",
    "128": "assets/icons/icon128.png"
  },
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "https://labs.google/*",
    "https://firestore.googleapis.com/*",
    "https://veo3automator.firebaseapp.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://labs.google/*"
      ],
              "js": [
                "src/license/device-fingerprint.js",
                "src/license/firebase-api.js",
                "src/license/license-system.js",
                "src/utils/find-replace-utils.js",
                "src/core/content.js",
                "src/automation/prompt-organizer.js",
                "src/automation/image-automator.js",
                "src/automation/whisk-automator.js",
                "src/automation/auto-image-loader.js"
              ],
      "css": [
        "src/interface/sidebar.css"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "src/interface/sidebar.html",
        "src/interface/troubleshooting.html",
        "src/license/license-activation.html",
        "src/license/license-activation-script.js",
        "src/license/device-fingerprint.js",
        "src/license/firebase-api.js",
        "src/license/license-system.js",
        "src/automation/prompt-organizer.js",
        "assets/icons/*"
      ],
      "matches": [
        "https://labs.google/*"
      ]
    }
  ]
}